       <notes><![CDATA[ file name: okta-sdk-httpclient-1.0.1-SNAPSHOT.jar ]]></notes>
       <gav regex="true">^com\.okta\.sdk:okta-sdk-httpclient:.*$</gav>
       <cpe>cpe:/a:rest-client_project:rest-client</cpe>
